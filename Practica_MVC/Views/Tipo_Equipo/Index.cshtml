@model IEnumerable<Practica_MVC.Models.Tipo_EquipoCLS>

@{
    ViewBag.Title = "Tipo equipo";
}

<h2>Tipo equipo</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.tipo_equipo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.medida)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.tipo_equipo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.medida)
        </td>
    </tr>
}

</table>
