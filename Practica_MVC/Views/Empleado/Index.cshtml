@model IEnumerable<Practica_MVC.Models.EmpleadoCLS>

@{
    ViewBag.Title = "Empleado";
}

<h2>Empleados</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.id_empleado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.salario_por_hora)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nombre)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.id_empleado)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.salario_por_hora)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.nombre)
        </td>
    </tr>
}

</table>
